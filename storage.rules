rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Archivos de odont√≥logos: solo usuarios autenticados
    match /odontologos/{odontologoId}/{allPaths=**} {
      allow read: if request.auth != null;
      allow write: if request.auth != null && request.auth.uid == odontologoId;
    }
  }
}
